    </collision>
    
    <collision>
      <origin rpy="-1.57079632679 0 0" xyz="0 0.045 0"/>
      <geometry>
        <cylinder length="0.07" radius = "0.06"/>
      </geometry>
    </collision>

    <collision>
      <origin rpy="0.0 0 0" xyz="-0.03 0.045 0"/>
      <geometry>
        <box size="0.06 0.07 0.11"/>
      </geometry>
    </collision>  

    <collision>
      <origin rpy="-1.57079632679 0 0" xyz="0 0.045 0"/>
      <geometry>
        <cylinder length="0.07" radius = "0.06"/>
      </geometry>
    </collision>

    <visual>
      <origin rpy="1.57079632679 0 1.57079632679" xyz="-0.515 0 0"/>
      <geometry>
        <mesh filename="package://sr1_description/mesh/3_axis.dae"/>
      </geometry>
    </visual>
    
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia
	  ixx="1.0" ixy="0.0" ixz="0.0"
	  iyy="1.0" iyz="0.0"
	  izz="1.0"/>
    </inertial>
  </link>
  
  <!-- Joint 4-->
  <joint name="joint_4" type="revolute">
