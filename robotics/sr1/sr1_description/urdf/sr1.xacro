    <limit lower="-2.2689280275926284500007979990352" upper="2.2689280275926284500007979990352" effort="1000.0" velocity="0.1"/>
  </joint>
  
  <!-- Link 5-->
  <link name="link_5">
    <collision>
      <origin rpy="1.57079632679 0 0" xyz="0 -0.025 0"/>
