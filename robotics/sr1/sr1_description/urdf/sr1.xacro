    <dynamics damping="0.7"/>
    <limit lower="-5.061454830783555773078703228617" upper="1.832595714594046055769875306913" effort="1000.0" velocity="0.1"/>
  </joint>
  
  <!-- Link 3-->
  <link name="link_3">
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius = "0.05"/>
