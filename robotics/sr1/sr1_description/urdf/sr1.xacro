#  <xacro:property name="mesh_dir" value="/home/i5/" />
  <!-- Import Rviz colors -->


  <!-- Used for fixing robot to Gazebo 'base_link' -->
  
  <link name="world"/>

  <joint name="joint_base" type="fixed">
    <parent link="world"/>
    <child link="link_base"/>
    <origin xyz="0 0 ${joint_offset_base}" rpy="0 0 0"/>
  </joint>
  
    <!-- Link base-->
  <link name="link_base">

    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.01" radius = "0.09"/>
      </geometry>
    </collision>

    <collision>
     <origin rpy="0.0 0 0" xyz="0 0 0.085"/>
      <geometry>
        <cylinder length="0.15" radius = "0.06"/>
      </geometry>
    </collision>

    <collision>
     <origin rpy="0.0 0 0" xyz="-0.055 0 0.045"/>
      <geometry>
        <box size= "0.02 0.09 0.09"/>
      </geometry>
    </collision>

    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sr1_description/mesh/base.dae"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia
	  ixx="1.0" ixy="0.0" ixz="0.0"
	  iyy="1.0" iyz="0.0"
	  izz="1.0"/>
    </inertial>
  </link>

  <joint name="joint_1" type="revolute">
    <parent link="link_base"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 ${joint_offset_1}" rpy="0 0 0"/>
    <dynamics damping="0.7"/>
    <limit lower="-2.7925268031909273230779052295818" upper="2.53177" effort="1000.0" velocity="0.1"/>
  </joint>

  <!-- Link 1-->
  <link name="link_1">
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0.22"/>
      <geometry>
        <box size="0.1 0.11 0.13"/>
      </geometry>      
    </collision>

    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://sr1_description/mesh/1_axis.dae"/>
      </geometry>      
    </visual>

    <inertial>
      <origin xyz="0 0 ${j2l_offset1}" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia
	  ixx="1.0" ixy="0.0" ixz="0.0"
	  iyy="1.0" iyz="0.0"
	  izz="1.0"/>
    </inertial>
  </link>

  <!-- Joint 2-->
  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 ${joint_offset_2_z}" rpy="${-PI/2.0} 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.7"/>
    <limit lower="-1.9198621771937625346160598453375" upper="1.5707963267948966192313216916398" effort="1000.0" velocity="0.1"/>
  </joint>

  <!-- Link 2-->
  <link name="link_2">
    <collision>
      <origin rpy="0.0 0 0" xyz="0 -0.15 0"/>
      <geometry>
        <box size="0.06 0.11 0.12"/>
      </geometry>
    </collision>
    
    <collision>
      <origin rpy="0.0 0 0" xyz="0 -0.13 0.075"/>
      <geometry>
